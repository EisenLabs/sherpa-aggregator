  "scripts": {
    "compile": "forge compile",
    "size-contracts": "SKIP_LOAD=true yarn hardhat size-contracts",
    "coverage:forge": "forge coverage --match-contract UnitTest --report lcov",
    "gas:hardhat": "REPORT_GAS=true yarn hardhat test",
    "precommit": "pre-commit install",
    "prettier:fix": "prettier --write '**/*.ts' && prettier --write '**/*.json' && forge fmt",
    "prettier:ts": "prettier --check \"{,!(node_modules)/**/}*.ts\"",
    "prettier:fix:ts": "prettier --write \"{,!(node_modules)/**/}*.ts\"",
    "prettier": "prettier --check '**/*.ts' && forge fmt --check",
    "lint:sol": "solhint -q 'contracts/**/*.sol' && prettier --check contracts/**/*.sol",
    "lint:fix:sol": "prettier --write 'contracts/**/*.sol' && solhint 'contracts/**/*.sol' --fix",
    "postinstall": "patch-package",
    "prepare": "yarn prepare:husky",
    "prepare:husky": "husky install",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "abi:generate": "make clean && forge build --skip script --skip test --skip Base --skip Test && hardhat diamondABI",
    "codegen": "plop"
  }
}
